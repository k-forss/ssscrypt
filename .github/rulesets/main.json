    {
      "type": "required_status_checks",
      "parameters": {
        "strict_status_checks_policy": true,
        "required_status_checks": [
          {
            "context": "Check & Lint",
            "integration_id": 15368
          },
          {
            "context": "Test",
            "integration_id": 15368
          },
          {
            "context": "Dependency Checks",
            "integration_id": 15368
          }
        ]
      }
    },
    {
      "type": "non_fast_forward"
    },
    {
      "type": "deletion"
    }
  ]
}
